{
  "name": "sword-task-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "docker:start": "docker-compose up -d",
    "test": "jest --watchAll --verbose",
    "seed": "node prisma/seed.js",
    "migrate:dev": "prisma migrate reset -f",
    "docker:migrate": "docker-compose -f ../docker-compose.yml run --rm sword_task_api npm run migrate:dev",
    "docker:seed": "docker-compose -f ../docker-compose.yml run --rm sword_task_api npm run seed"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^3.4.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv-cli": "^4.1.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "faker": "^5.5.3",
    "jest": "^27.3.1",
    "prisma": "^3.4.0",
    "supertest": "^6.1.6"
  }
}
